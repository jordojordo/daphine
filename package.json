{
  "name": "daphine",
  "version": "0.4.1",
  "description": "The API for Freedom",
  "author": "Jordon Leach",
  "license": "GPL-3.0-only",
  "main": "server.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jordojordo/daphine.git"
  },
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/views/**/* dist",
    "start": "node ./dist/server.js",
    "start:dev": "tsc-watch --onSuccess \"pnpm run serve\"",
    "serve": "pnpm run copy-views && pnpm run start",
    "copy-views": "copyfiles -u 1 src/views/**/* dist",
    "lint": "npx eslint .",
    "format": "npx eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/http-errors": "^2.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.1",
    "cookie-parser": "^1.4.7",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "eslint": "^9.34.0",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.1",
    "puppeteer": "^24.19.0",
    "tsc-watch": "^7.1.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  },
  "_moduleAliases": {
    "@src": "dist",
    "@routes": "dist/routes",
    "@api": "dist/api"
  }
}
