{
  "name": "daphine",
  "version": "0.2.0",
  "description": "The API for Freedom",
  "author": "Jordon Leach",
  "license": "GPL-3.0-only",
  "main": "server.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jordojordo/daphine.git"
  },
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/views/**/* dist",
    "start": "node ./dist/server.js",
    "start:dev": "tsc-watch --onSuccess \"npm run serve\"",
    "serve": "npm run copy-views && npm run start",
    "copy-views": "copyfiles -u 1 src/views/**/* dist",
    "lint": "npx eslint .",
    "format": "npx eslint . --fix"
  },
  "dependencies": {
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.5",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.9",
    "eslint": "^9.0.0",
    "express": "^4.19.2",
    "globals": "^15.0.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "puppeteer": "^22.6.3",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.4"
  },
  "_moduleAliases": {
    "@src": "dist",
    "@routes": "dist/routes",
    "@api": "dist/api"
  }
}
